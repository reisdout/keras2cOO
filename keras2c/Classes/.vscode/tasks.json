{
    "tasks": [
        {
            "type": "cppbuild",
            "label": "C/C++: g++-11 build active file",
            "command": "/usr/bin/g++-11",
            "args": [
                "-fdiagnostics-color=always",                
                "${file}",
                "${fileDirname}/src/*.cpp",
                "-I${fileDirname}/include/*.h",
                "-I${fileDirname}/Model/*.h",
                "${fileDirname}/Model/include/*.c",
                "-I${fileDirname}/Model/include/*.h",
                "${fileDirname}/MLP_3000_epocas/*.cpp",
                "-I${fileDirname}/MLP_3000_epocas/*.h",
                "${fileDirname}/LSTM_3000_epocas/*.cpp",
                "-I${fileDirname}/LSTM_3000_epocas/*.h",
                "${fileDirname}/CNN_3000_epocas/*.cpp",
                "-I${fileDirname}/CNN_3000_epocas/*.h",
                "${fileDirname}/MLP_3000_epocas_SND_RTT/*.cpp",
                "-I${fileDirname}/MLP_3000_epocas_SND_RTT/*.h",
                "-L${fileDirname}/Model/include/",
                "-l:libkeras2c.a",
                "-lm",
                "-o",
                "${fileDirname}/${fileBasenameNoExtension}"
            ],
            "options": {
                "cwd": "${fileDirname}"
            },
            "problemMatcher": [
                "$gcc"
            ],
            "group": {
                "kind": "build",
                "isDefault": true
            },
            "detail": "Task generated by Debugger."
        }
    ],
    "version": "2.0.0"
}